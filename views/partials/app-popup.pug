.popup-wrapper
  .arcade-title-popup
    div arcade.studio
  .app-popup
    .app-popup-menu
      a.btn-launch.for-mobile(href="#" target="_blank")
        img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/website/master/item-side-menu-options/published-project.svg" width="22")
      a.btn-launch.for-desktop(href="#")
        img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/website/master/fullscreen.svg" width="20")
      a.btn-share(href="#")
        img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/website/master/item-side-menu-options/social-share-dots.svg" width="20")
    .popup-body
      .iframe-wrapper
        .app-popup-share
          .popup-social-content
            .popup-social-div.icons
              .popup-social-icons
                .social-icon-div
                  img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-facebook.svg" class="shareOnFacebook social-icon-div" alt="facebook" title="facebook")
                .social-icon-div
                  img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-twitter.svg" class="shareOnTwitter social-icon-div" alt="twitter" title="twitter")
                .social-icon-div
                  img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-reddit.svg" class="shareOnReddit social-icon-div" alt="reddit" title="reddit")
                .social-icon-div
                  img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-linkedin.svg" class="shareOnLinkedin social-icon-div" alt="linkedin" title="linkedin")
                .social-icon-div
                  img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-pinterest.svg" class="shareOnPinterest social-icon-div" alt="pinterest" title="pinterest")
                .social-icon-div
                  a.shareViaEmail
                    img(src="https://arcadestudio-assets.s3.us-east-2.amazonaws.com/social/social-email.svg" class="shareOnEmail social-icon-div" alt="email" title="email")
            .popup-social-div-wrapper
              .popup-social-div
                .social-url-copy-title Link
                .social-url-copy-link
              .popup-social-div.middle
                .social-embed-text-title Embed
                .social-embed-text-copy
            .popup-social-div.last
              .social-close-btn-text close
              .social-copied-btn-text COPIED TO CLIPBOARD
        iframe(src="")
      .project-detail
      .project-author
      .project-description
